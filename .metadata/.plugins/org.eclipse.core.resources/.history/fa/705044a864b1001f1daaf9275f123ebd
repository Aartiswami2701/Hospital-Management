spring.application.name=Spring-security-config-cloud-doctor
server.port= 6000
 
# --------------Externialization using Git-------------
spring.profiles.active=git
spring.cloud.config.server.git.uri = https://github.com/fortrainees2020/spring-config-repo/blob/master/application.properties
spring.cloud.config.server.branch = master
spring.cloud.config.server.git.clone-on-start=true
