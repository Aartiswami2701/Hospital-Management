spring.application.name=Spring-security-config-cloud-doctor
server.port=6000

# Externalize configuration using Git
spring.profiles.active=git
spring.cloud.config.server.git.uri=https://github.com/fortrainees2020/spring-config-repo.git
spring.cloud.config.server.branch=master
spring.cloud.config.server.git.clone-on-start=true
spring.datasource.url=jdbc:h2:mem:testdb
spring.datasource.driver-class-name=org.h2.Driver
